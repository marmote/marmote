<block>
  <name>Marmote MAC framer</name>
  <key>marmote_mac_framer_b</key>
  <category>MarmotE</category>
  <import>import marmote</import>
  <make>marmote.mac_framer_b($debug)</make>

    <param>
    <name>Debug</name>
    <key>debug</key>
    <value>False</value>
    <type>bool</type>

    <option>
        <name>Enable</name>
        <key>True</key>
    </option>
    <option>
        <name>Disable</name>
        <key>False</key>
    </option>
    </param>

    <sink>
        <name>in</name>
        <type>message</type>
    </sink>

  <source>
    <name>out</name>
    <type>byte</type>
    <nports>1</nports>
  </source>
</block>
