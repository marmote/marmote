<?xml version="1.0"?>
<block>
    <name>PN acquisition</name>
    <key>marmote_pn_acquisition</key>
    <category>MarmotE</category>
    <import>import marmote</import>
    <make>marmote.pn_acquisition(
        preamble_len=$preamble_len, 
        $spread_factor, 
        $oversample_factor, 
        $pn_mask, 
        $pn_seed
        <!--spread_factor=$spread_factor, -->
        <!--oversample_factor=$oversample_factor, -->
        <!--pn_mask=$pn_mask, -->
        <!--pn_seed=$pn_seed-->
        )</make>
    <param>
        <name>Preamble length</name>
        <key>preamble_len</key>
        <type>int</type>
    </param>

    <sink>
        <name>in</name>
        <type>int</type>
    </sink>

    <source>
        <name>out</name>
        <type>int</type>
    </source>
</block>
