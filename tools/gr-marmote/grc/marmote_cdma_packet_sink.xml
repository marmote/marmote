<?xml version="1.0"?>
<block>
  <name>CDMA packet sink</name>
  <key>marmote_cdma_packet_sink</key>
  <category>MarmotE</category>
  <import>import marmote</import>
  <make>marmote.cdma_packet_sink(debug=$debug, id=$node_id, total_pkt=$total_pkt)</make>

  <param>
    <name>Debug</name>
    <key>debug</key>
    <value>False</value>
    <type>bool</type>
    <option>
      <name>Enable</name>
      <key>True</key>
    </option>
    <option>
      <name>Disable</name>
      <key>False</key>
    </option>
  </param>

  <param>
    <name>Node ID</name>
    <key>node_id</key>
    <type>int</type>
  </param>

  <param>
    <name>Total packets</name>
    <key>total_pkt</key>
    <value>1000</value>
    <type>int</type>
  </param>

  <sink>
    <name>in</name>
    <type>message</type>
  </sink>

</block>
