<?xml version="1.0"?>
<block>
  <name>CDMA packet source</name>
  <key>marmote_cdma_packet_source</key>
  <category>MarmotE</category>
  <import>import marmote</import>
  <make>marmote.cdma_packet_source(
    debug=$debug,
    payload_len=$payload_len)</make>

  <param>
    <name>Debug</name>
    <key>debug</key>
    <value>False</value>
    <type>bool</type>
    <option>
      <name>Enable</name>
      <key>True</key>
    </option>
    <option>
      <name>Disable</name>
      <key>False</key>
    </option>
  </param>

  <param>
    <name>Payload length (bytes)</name>
    <key>payload_len</key>
    <value>32</value>
    <type>int</type>
  </param>

  <sink>
    <name>in</name>
    <type>message</type>
  </sink>

  <source>
    <name>out</name>
    <type>message</type>
  </source>

  <doc>
    This is a simple packet source for the CDMA project.
  </doc>
</block>
